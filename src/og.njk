      {% endfor %}
    </text>

    <text font-family='ui-sans-serif, system-ui, -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, "Noto Sans", sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol", "Noto Color Emoji"' font-size="30" font-weight="500" fill="rgb(221, 214, 254)">
      {% set descriptionLines = preview.data.description | default(preview.templateContent | excerpt | striptags | decode_entities) | splitlines %}
      {% for line in descriptionLines %}
      <tspan x="100" y="{{350 + 80 * titleLines.length + loop.index0 * 40}}">{{line}}</tspan>
      {% endfor %}
    </text>
